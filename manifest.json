{
    "manifest_version": 2,
    "name": "Dark Thumbnail Extension",
    "version": "1.0.0",
    "description": "Extension to make the thumbnail of videos that you watched on youtube become dark.",
    "icons": {
        "16": "icons/dark_extension_16.png",
        "48": "icons/dark_extension_48.png",
        "128": "icons/dark_extension_16.png"
    },
    "author": "Albert Regus - RegusAl",
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/"],
            "css": ["style.css"]
        }
    ]
}